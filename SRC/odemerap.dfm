  object queryODEME: TQuery
    Active = True
    DatabaseName = 'BM'
    SQL.Strings = (
      'SELECT * FROM ODEME'
      
        'WHERE ( (ODEME_TARIHI >= :TARIH1 AND ODEME_TARIHI <= :TARIH2) AN' +
        'D'
      '                 (MNO >= :MNO1 AND MNO <= :MNO2) AND'
      '                 (SATICI >= :SATICI1 AND SATICI <= :SATICI2) )')
    Left = 56
    Top = 8
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'TARIH1'
        ParamType = ptUnknown
      end
      item
        DataType = ftDateTime
        Name = 'TARIH2'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'MNO1'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'MNO2'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'SATICI1'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'SATICI2'
        ParamType = ptUnknown
      end>
  end
